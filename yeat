function addspectate(v)
    v.MouseEnter:Connect(function()
        local key = Instance.new("Folder")
        key.Name = "CanSpectate"
        key.Parent = v
    end)
    v.MouseLeave:Connect(function()
        local key = v:FindFirstChild("CanSpectate")
        if key then
            key:Destroy()
        end
    end)
end

replaceleaderscript()

task.wait(0.25)

for i,v in ipairs(leaderboard:GetChildren()) do
    v.Position = UDim2.new(0, 0, 0, (i - 1) * 20);
	v.Visible = true;
    v:GetPropertyChangedSignal("Text"):Connect(function()
        print(v.Text)
    end)
    addspectate(v)
end

task.spawn(function()
    while task.wait(5) do
        replaceleaderscript()

task.wait(0.25)

for i,v in ipairs(leaderboard:GetChildren()) do
    v.Position = UDim2.new(0, 0, 0, (i - 1) * 20);
	v.Visible = true;
    v:GetPropertyChangedSignal("Text"):Connect(function()
        print(v.Text)
    end)
    addspectate(v)
end
    end
end)

leaderboard.ChildAdded:Connect(function(v)
    local amount = leaderboard:GetChildren()
    v.Position = UDim2.new(0, 0, 0, (amount - 1) * 20);
	v.Visible = true;
    task.wait()
    addspectate(v)
end)

Mouse.Button1Down:Connect(function()
    for i,v in pairs(leaderboard:GetChildren()) do
        if v:FindFirstChild("CanSpectate") then
            local oldname = v.Text
            local name = string.gsub(oldname, "[ ?]", "")
            local plr = game.Players:FindFirstChild(name)
            if plr then
                if not plr.Character:FindFirstChild("Humanoid") then
                    continue
                end
                workspace.CurrentCamera.CameraSubject = plr.Character.Humanoid
            else
                local oldname = v.Text
                local name = string.gsub(oldname, "[ ?]", "")
                print(name.." not found")
            end
        end
    end
end)
