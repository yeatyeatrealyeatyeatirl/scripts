end)

misc:Slider("FOV",{
    min = 50; -- min value of the slider
    max = 120; -- max value of the slider
    precise = true; -- max 2 decimals
},function(value)
    game:GetService("Workspace").CurrentCamera.FieldOfView = value
end)

